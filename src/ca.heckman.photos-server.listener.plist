<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>ca.heckman.photos-server</string>

        <key>Program</key>
        <string>/usr/local/libexec/photos-http-handler</string>

        <key>Sockets</key>
        <dict>
            <key>Photos</key>
            <dict>
                <key>SockNodeName</key>
                <string>localhost</string>
                <key>SockServiceName</key>
                <string>6330</string>
                <key>SockPassive</key>
                <true/>
            </dict>
        </dict>
        <key>inetdCompatibility</key>
        <dict>
            <key>Wait</key>
            <false/>
        </dict>

        <key>StandardErrorPath</key>
        <string>/Users/erik/dev/project.photos-server/photos-server/log/launchagent-error.log</string>

        <key>RunAtLoad</key>
        <true/>

    </dict>
</plist>
