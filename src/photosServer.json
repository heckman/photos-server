{
	"osabundle": "0.1",
	"name": "Photos Server",
	"version": "0.1",
	"identifier": "ca.heckman.PhotosServer",
	"copyright": "Copyright 2026 Erik Ben Heckman <erik@heckman.ca>",
	"target": "/dist/Photos Server.app",
	"entry": "photosServer.jxa",
	"prescript": [
		"const predicate = (url) => url.host.js == \"photos.local\";",
		"const defaultOpenCommand = \"open -a Safari\";"
	],
	"icon": "icon/AppIcon.icon",
	"legacyIcon": "icon/Sequoia/light.png",
	"contents": "contents",
	"plist": {
		"CFBundleDocumentTypes": [
			{
				"CFBundleTypeName": "HTML document",
				"CFBundleTypeRole": "Viewer",
				"LSItemContentTypes": ["public.html"]
			},
			{
				"CFBundleTypeName": "XHTML document",
				"CFBundleTypeRole": "Viewer",
				"LSItemContentTypes": ["public.xhtml"]
			}
		],
		"CFBundleURLTypes": [
			{
				"CFBundleURLName": "Web site URL",
				"CFBundleURLSchemes": ["http", "https"]
			}
		]
	}
}
