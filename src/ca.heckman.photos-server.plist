<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
    <dict>
        <key>Label</key>
        <string>ca.heckman.photos-server</string>

        <key>Program</key>
        <string>/usr/local/libexec/photos-http-response-handler</string>

        <key>Sockets</key>
        <dict>
            <key>Photos</key>
            <dict>
                <key>SockNodeName</key>
                <string>127.0.63.30</string>
                <key>SockServiceName</key>
                <string>80</string>
                <key>SockPassive</key>
                <true/>
            </dict>
        </dict>
        <key>inetdCompatibility</key>
        <dict>
            <key>Wait</key>
            <false/>
        </dict>

        <key>StandardErrorPath</key>
        <string>/dev/null</string>

    </dict>
</plist>
